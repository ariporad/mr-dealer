{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Server Tests",
			"type": "node",
			"request": "launch",
			"runtimeArgs": [
				"--inspect-brk",
				"${workspaceRoot}/server/node_modules/.bin/jest",
				"--runInBand"
			],
			"cwd": "${workspaceRoot}/server",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"port": 9229
		}
	]
}
